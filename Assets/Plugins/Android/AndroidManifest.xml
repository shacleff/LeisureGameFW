<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="martian.com.unitypluginslib" xmlns:tools="http://schemas.android.com/tools">
	<uses-permission android:name="android.permission.VIBRATE" ></uses-permission>
  <application android:theme="@style/UnityThemeSelector"
				android:label="@string/app_name">
    <activity android:name="martian.com.unitypluginslib.VibrateUtil" android:label="@string/app_name">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
      
    </activity>
    
  </application>
</manifest>